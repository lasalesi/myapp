 "html": "<!-- HEAD -->\n<div  id=\"header-html\" class=\"hidden-pdf\" >\n  <table style=\"width: 100%;\">\n    <tr>\n      <td>SEG</td>\n      <td style=\"text-align: right;\">Sales Report {{ doc.get_formatted('date') }}</td>\n    </tr>\n  </table>\n</div>\n<meta name=\"pdfkit-orientation\" content=\"Landscape\"/>\n<div id=\"footer-html\" class=\"visible-pdf\">\n  <table style=\"width:100%;\" >\n    <tr>\n      <td style=\"width: 30%;\">libracore business software</td>\n      <td style=\"text-align: center;\">Page <span class=\"page\"></span> of <span class=\"topage\"></span></span>\n      <td style=\"width:30%; text-align: right;\">https://www.libracore.com</span></p>\n    </tr>\n  </table>\n</div>\n\n<!-- CONTENT -->\n<center><h3>Sales Report</h3></center>\n<table style=\"width: 100%;\">\n  <tr>\n    <th>Ende KW {{ doc.week }}</th>\n    <th colspan=2><center>Aktuelle Woche</center></th>\n    <th colspan=2><center>Kumuliert {{ doc.this_year }}</center></th>\n    <th colspan=2><center>Rolling demand/Woche {{ doc.this_year }}</center></th>\n    <th colspan=2><center>Rolling demand/Woche {{ doc.last_year }}</center></th>\n    <th colspan=2><center>Kumuliert {{ doc.last_year }}</center></th>\n  </tr>\t\n  <tr>\n    <th>Produkt</th>\n    <th style=\"text-align:right; \">Kg</th>\n    <th style=\"text-align:right; \">Umsatz CHF</th>\n    <th style=\"text-align:right; \">Kg</th>\n    <th style=\"text-align:right; \">Umsatz kum.</th>\n    <th style=\"text-align:right; \">Kg</th>\n    <th style=\"text-align:right; \">Umsatz</th>\n    <th style=\"text-align:right; \">Kg</th>\n    <th style=\"text-align:right; \">CHF</th>\n    <th style=\"text-align:right; \">Kg</th>\n    <th style=\"text-align:right; \">CHF</th> \n  </tr>\n  {% for n in doc.items %}\n  <tr>\n    <td style=\"padding: 1px !important; \">{{ n.description }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ n.qty_7days }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(n.revenue_7days).replace(\",\", \"'\") }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ n.qty_ytd }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(n.revenue_ytd).replace(\",\", \"'\") }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ n.demand_qty_ytd }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(n.demand_revenue_ytd).replace(\",\", \"'\") }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ n.demand_qty_py }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(n.demand_revenue_py).replace(\",\", \"'\") }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ n.qty_py }}</td>\n    <td style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(n.revenue_py).replace(\",\", \"'\") }}</td>\n  </tr>\n  {% endfor %} \n  <tr>\n    <th style=\"padding: 1px !important; \">Summe</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ doc.total_qty_7days }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(doc.total_revenue_7days).replace(\",\", \"'\") }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ doc.total_qty_ytd }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(doc.total_revenue_ytd).replace(\",\", \"'\") }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ doc.total_demand_qty_ytd }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(doc.total_demand_revenue_ytd).replace(\",\", \"'\") }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ doc.total_demand_qty_py }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(doc.total_demand_revenue_py).replace(\",\", \"'\") }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ doc.total_qty_py }}</th>\n    <th style=\"text-align:right; padding: 1px !important; \">{{ \"{:,.0f}\".format(doc.total_revenue_py).replace(\",\", \"'\") }}</th>\n  </tr>\n</table>\n<p>{{ doc.remarks or '' }}</p>", 
